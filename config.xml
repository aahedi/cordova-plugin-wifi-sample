<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gurvanhenry.cordova.wifi.sample" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-plugin-wifi-sample</name>
    <author>Gurvan Henry</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.4.0" />
    <plugin name="cordova-plugin-file" spec="4.2.0"/>
    <plugin name="cordova-plugin-file-transfer" spec="1.5.1"/>
    <plugin name="cordova-plugin-inappbrowser" spec="1.4.0"/>
    <plugin name="cordova-plugin-x-socialsharing" spec="https://github.com/EddyVerbruggen/SocialSharing-PhoneGap-Plugin"/>
    <plugin name="com.guinatal.refreshgallery" spec="https://github.com/guinatal/refreshgallery.git"/>
    <plugin name="com.gurvanhenry.cordova-plugin-wifi" spec="^1.0.1" />
    <plugin name="com.xonoxitron.cordova-plugin-hotspot" spec="https://github.com/xonoxitron/cordova-plugin-hotspot.git" />
    <plugin name="com.hypery2k.cordova-plugin-hotspot" spec="https://github.com/hypery2k/cordova-hotspot-plugin.git" />
    <plugin name="com.floatinghotpot.cordova-plugin-httpd" spec="https://github.com/floatinghotpot/cordova-httpd.git" />
    <plugin name="com.moonware.cordova-plugin-cameraserver" spec="https://github.com/aahedi/cordova-cameraserver.git" />
    <plugin name="com.katzer.cordova-plugin-background-mode" spec="https://github.com/katzer/cordova-plugin-background-mode.git" />
    <plugin name="com.phonegap.cordova-plugin-contentsync" spec="https://github.com/phonegap/phonegap-plugin-contentsync.git" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
